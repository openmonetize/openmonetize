{
  "name": "@openmonetize/sdk",
  "version": "0.8.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "description": "Official TypeScript SDK for OpenMonetize - AI usage tracking and billing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "lint": "eslint src/",
    "version:patch": "pnpm version patch && pnpm build",
    "version:minor": "pnpm version minor && pnpm build",
    "version:major": "pnpm version major && pnpm build",
    "release:patch": "pnpm version:patch && npm publish --access public",
    "release:minor": "pnpm version:minor && npm publish --access public",
    "release:major": "pnpm version:major && npm publish --access public"
  },
  "keywords": [
    "openmonetize",
    "ai",
    "billing",
    "usage-tracking",
    "metering",
    "credits",
    "openai",
    "anthropic"
  ],
  "author": "OpenMonetize",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/uuid": "9",
    "tsup": "^8.0.1",
    "typescript": "^5.9.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
